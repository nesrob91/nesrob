package com.elektra.cadsumutils.generales;

/**
 *
 * @author dparra
 */
public class Generales {

    public String substr(String string, int min, int max) {

        if (string == null) {
            return string;
        } else {
            if (string.length() > max) {
                return string.substring(min, max);
            } else {
                return string;
            }
        }

    }

}
